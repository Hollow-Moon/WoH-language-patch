 */
kt.dashcombo	= function(elm)
{
	//	次に wdash を実行することにして
	elm.acttype = "dash";
	elm.module = "MixedKeyFrameActionModule";
	elm.tagname	= "wdash";
	conductor.pendings.insert(0, elm);

	return tagHandlers.dash(elm);	//	dash を実行
} incontextof kag;

/**TAG
