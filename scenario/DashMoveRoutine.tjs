kt.ld_notrans	= function(mp)
{
	with(mp)
	{
		if(.file == "" && .storage == "")
			throw new Exception("fileが指定されていません。");
		.pos	= "center" if .pos == "" && .left === void && .center === void;	//	指定が無ければ中央
