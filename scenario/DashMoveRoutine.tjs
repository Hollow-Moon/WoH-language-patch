kt.splinemovecombo	= function(elm)
{
	//	次に wsplinemove を実行することにして
//	elm.seqid = -1;
	elm.acttype = "dash";
	elm.tagname	= "wsplinemove";
	conductor.pendings.insert(0, elm);

	return tagHandlers.splinemove(elm);	//	splinemove を実行
} incontextof kag;

